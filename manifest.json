{
	"name": "SkipAds YT",
	"description": "Skip ads on youtube with one button. (default Crtl+End)",
	"author": {
		"email": "user@example.com"
	},
	"version": "2.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	},
	"permissions": ["scripting", "activeTab"],
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+End",
				"mac": "Command+End"
			}
		}
	}
}
